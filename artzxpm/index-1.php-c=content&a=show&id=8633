<script>

   function GetRTime(){

       var EndTime= new Date('2018/12/16 14:00:00');

       var NowTime = new Date();

       var t =EndTime.getTime() - NowTime.getTime();

	     if(t > 0){ 





       var d=Math.floor(t/1000/60/60/24);

       var h=Math.floor(t/1000/60/60%24);

       var m=Math.floor(t/1000/60%60);

       var s=Math.floor(t/1000%60);

 var ms = Math.floor(t00);



       document.getElementById("t_d").innerHTML = d + "<font size='3px'>天</font>";

       document.getElementById("t_h").innerHTML = h + "<font size='3px'>时</font>";

       document.getElementById("t_m").innerHTML = m + "<font size='3px'>分</font>";

       document.getElementById("t_s").innerHTML = s + "<font size='3px'>秒</font>";

	    document.getElementById("h_s").innerHTML = ms;

		}

		else

		{

		 document.getElementById("t_d").innerHTML ="<font size='3px'>　　<b>拍卖已经截止</b></font>";

       document.getElementById("t_h").innerHTML ="<font size='3px'> </font>";

       document.getElementById("t_m").innerHTML ="<font size='3px'> </font>";

       document.getElementById("t_s").innerHTML = "<font size='3px'> </font>";

	    document.getElementById("h_s").innerHTML = " ";

		}

   }

   setInterval(GetRTime,0);

