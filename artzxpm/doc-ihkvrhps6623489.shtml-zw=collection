<script type="text/javascript">    

$(document).scroll(function(){ 

	var  scrollTop =  $(document).scrollTop(),bodyHeight = $(window).height(); 

	if(scrollTop > bodyHeight){ 

		$('.fixed-bar .gotop').css('display','block');

	}else{

		$('.fixed-bar .gotop').css('display','none');

	} 

})

</script>





 





